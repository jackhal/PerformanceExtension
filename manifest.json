{
  "manifest_version": 3,
  "name": "Performance Monitor",
  "description": "Gives System and Tab Information",
  "version": "1.0",
  "permissions": ["system.cpu", "system.memory", "system.display", "tabs", "processes"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png"
  }
}